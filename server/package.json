{
  "name": "read-dojo-server",
  "version": "1.0.0",
  "description": "Keep track of all your awesomical urls and read-your-things like a pro",
  "main": "index.js",
  "scripts": {
    "start": "node index",
    "test": "jest  --verbose ./**/*.test.js",
    "test:coverage": "jest --verbose ./**/*.test.js --coverage",
    "test:watch": "jest --verbose ./**/*.test.js --watchAll",
    "lint": "standard",
    "lint:fix": "standard --fix",
    "dev": "nodemon ./lib/index.js",
    "debug": "DEBUG=* node index",
    "infra:start": "docker-compose up -d",
    "infra:stop": "docker-compose down"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KoolTheba/read-dojo.git"
  },
